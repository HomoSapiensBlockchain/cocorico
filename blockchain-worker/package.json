{
  "name": "cocorico-queue",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "amqplib": "0.4.0",
    "async": "1.5.2",
    "babel-cli": "^6.14.0",
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.14.0",
    "callback-and-promise": "^2.0.0",
    "cocorico-logger": "../lib/cocorico-logger",
    "ethereumjs-tx": "^1.1.1",
    "ethereumjs-util": "^4.0.1",
    "md5": "2.1.0",
    "solc": "^0.4.10",
    "source-map-support": "^0.4.6",
    "thenify": "^3.2.1",
    "timeout-as-promise": "^1.0.0",
    "web3": "^0.18.4"
  },
  "engines": {
    "node": ">=0.10.22",
    "npm": ">=1.3.14"
  },
  "scripts": {
    "build-ballot": "babel src/ballot --out-dir dist/ballot --source-maps inline",
    "build-vote": "babel src/vote --out-dir dist/vote --source-maps inline",
    "vote": "service cocorico-blockchain-worker-vote stop ; nodemon -w src/vote -d 5  --exec 'npm run build-vote && nodejs ./dist/vote/index.js'",
    "ballot": "service cocorico-blockchain-worker-ballot stop ; nodemon -w src/ballot -d 5  --exec 'npm run build-ballot && nodejs ./dist/ballot/index.js'",
    "test": "eslint src"
  },
  "devDependencies": {
    "babel-eslint": "^6.1.2",
    "eslint": "^3.6.0"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-async-to-generator"
    ]
  }
}
