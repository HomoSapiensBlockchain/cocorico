{
  "name": "cocorico",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "babel-core": "^5.8.21",
    "babel-plugin-object-assign": "^1.2.1",
    "babelify": "^6.1.3",
    "browserify": "^11.2.0",
    "watchify": "latest",
    "browserify-middleware": "^7.0.0",
    "browserify-incremental": "latest",
    "keystone": "^0.3.13",
    "dotenv": "1.2.0",
    "model-transform": "^1.1.0",
    "react": "latest",
    "react-router": "latest",
    "react-bootstrap": "latest",
    "react-dom": "latest",
    "react-remarkable": "latest",
    "react-intl": "1.2.0",
    "react-document-title": "latest",
    "pace": "HubSpot/pace",
    "reflux": "latest",
    "request": "^2.60.0",
    "store-prototype": "^1.1.1",
    "underscore": "^1.8.3",
    "forever": "0.14.1",
    "d3": "3.5.6",
    "jquery": "2.1.4",
    "node-lessify": "latest",
    "passport": "latest",
    "oauth": "~0.9.12",
    "passport": "~0.2.1",
    "passport-oauth": "^1.0.0",
    "passport-openidconnect": "0.0.1",
    "lodash": "~2.4.1",
    "client-sessions": "latest",
    "bcrypt": "latest",
    "async": "latest",
    "node_redis": "latest",
    "node-metainspector": "latest"
  },
  "engines": {
    "node": ">=0.10.22",
    "npm": ">=1.3.14"
  },
  "scripts": {
    "start": "nodemon -w models -w routes -w templates -w keystone.js -w config -d 1 keystone.js",
    "build-app": "browserify -t babelify -t node-lessify --extension=.jsx app/script/index.js -o public/js/app.js",
    "watch-app": "watchify -v -t babelify -t node-lessify --extension=.jsx app/script/index.js --poll=2000 -o public/js/app.js"
  },
  "overrides": {
    "react": {
      "main": "react.js"
    }
  },
  "main": "keystone.js"
}
